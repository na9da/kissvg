{
    "name": "Kissvg",
    "short_name": "Kissvg",
    "version": "1.0",
    "description": "Convert web pages into SVG",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "kiss.png",
        "default_title": "Kissvg"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "tabs",
        "activeTab",
        "debugger",
        "downloads",
        "fontSettings",
        "http://*/",
        "https://*/"
    ]
}
